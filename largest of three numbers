#include<iostream>
using namespace std;
int main()
{
   int a,b,c;
   cout<<"enter the values of a,b,c:";
   cin>>a>>b>>c;
   if(a>b && b>c)
   {
   cout<<"a is largest number:";
   }
   else if(b>c && b>a)
   {
   cout<<"b is largest number:";
   }
   else
   {
   cout<<"c is largest number:";
   }
}   
